  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "unpkg-uri": "^0.1.0"
  },
  "devDependencies": {
    "chokidar": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
